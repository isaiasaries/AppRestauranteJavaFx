	<parameter name="FechaIni" class="java.lang.String"/>
	<parameter name="FechaFin" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT det_compras.`idProd`,
	sum( det_compras.cantidad) AS total,
	compras.fechacompra,
	productos.`descripcionProd`
FROM det_compras
	INNER JOIN compras ON 
	 compras.idcompras = det_compras.idcompras 
	INNER JOIN productos ON 
	 productos.`idProd` = det_compras.`idProd` 
WHERE	 DATE(compras.fechacompra) BETWEEN $P{FechaIni} AND $P{FechaFin}
GROUP BY det_compras.`idProd`]]>
	</queryString>
