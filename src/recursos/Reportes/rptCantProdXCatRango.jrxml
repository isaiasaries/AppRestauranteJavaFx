	<queryString language="SQL">
		<![CDATA[SELECT categorias.`idCategorias`,
	categorias.`descripcionCategorias`,
	det_orden.`idMenu`,
	menu.`descripcionMenu`,
	sum( det_orden.cantidad) AS vendidas,
	ordenes.fecha
FROM det_orden
	INNER JOIN menu ON 
	 menu.idmenu = det_orden.`idMenu` 
	INNER JOIN ordenes ON 
	 ordenes.`idOrdenes` = det_orden.`idOrdenes` 
	INNER JOIN categorias ON 
	 categorias.`idCategorias` = menu.`idCategorias`
	 WHERE 
	 DATE(ordenes.fecha) BETWEEN $P{FechaIni} AND $P{FechaFin} 
	 group by categorias.idCategorias, det_orden.idMenu
	 order by categorias.idCategorias, det_orden.idMenu]]>
	</queryString>
